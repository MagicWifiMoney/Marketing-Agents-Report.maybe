# Notion Integration Environment Variables
# Copy this file to .env and fill in your actual values

# Notion API Configuration
NOTION_API_KEY=your_notion_api_key_here
NOTION_PAGE_ID=your_notion_page_id_here

# Database IDs (set these after creating databases in Notion)
NOTION_CLIENTS_DB_ID=your_clients_database_id_here
NOTION_REPORTS_DB_ID=your_reports_database_id_here
NOTION_ACTION_ITEMS_DB_ID=your_action_items_database_id_here
NOTION_RESEARCH_DB_ID=your_research_database_id_here
NOTION_CONTENT_DB_ID=your_content_database_id_here
NOTION_METRICS_DB_ID=your_metrics_database_id_here

# Optional: Enable/disable Notion sync
ENABLE_NOTION_SYNC=true
